
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix opts:   <http://lv2plug.in/ns/ext/options#> .
@prefix time:   <http://lv2plug.in/ns/ext/time#>.
@prefix units:  <http://lv2plug.in/ns/extensions/units#> .
@prefix atom:   <http://lv2plug.in/ns/ext/atom#> .
@prefix urid:   <http://lv2plug.in/ns/ext/urid#> .
@prefix pprop:  <http://lv2plug.in/ns/ext/port-props#> .
@prefix midi:   <http://lv2plug.in/ns/ext/midi#> .
@prefix patch:  <http://lv2plug.in/ns/ext/patch#> .


<urn:name#me>
   a foaf:Person ;
   foaf:name "bill" .

<urn:bill:monospace>
   a lv2:Plugin ,
       lv2:MixerPlugin ;
   doap:maintainer <urn:name#me> ;
   doap:name "monospace" ;
   lv2:project <urn:bill:monospace> ;
   lv2:requiredFeature urid:map ;
   lv2:optionalFeature lv2:hardRTCapable ;
   lv2:minorVersion 1 ;
   lv2:microVersion 0 ;

guiext:ui <urn:bill:monospace_ui> ;

rdfs:comment """
...
""";


   lv2:port  [
       a lv2:AudioPort ,
          lv2:InputPort ;
      lv2:index 0 ;
      lv2:symbol "in0" ;
      lv2:name "In0" ;
   ], [
      a lv2:AudioPort ,
           lv2:OutputPort ;
      lv2:index 1 ;
      lv2:symbol "out0" ;
      lv2:name "Out0" ;
   ], [
      a lv2:AudioPort ,
           lv2:OutputPort ;
      lv2:index 2 ;
      lv2:symbol "out1" ;
      lv2:name "Out1" ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 3 ;
      lv2:symbol "DELAYMILLISECONDS" ;
      lv2:name "delaymilliseconds" ;
      lv2:default 20.000000 ;
      lv2:minimum 1.000000 ;
      lv2:maximum 40.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 4 ;
      lv2:symbol "IMAGEVOLUME" ;
      lv2:name "imagevolume" ;
      lv2:default 0.000000 ;
      lv2:minimum -96.000000 ;
      lv2:maximum 0.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 5 ;
      lv2:symbol "LOWPASSFILTERFREQ" ;
      lv2:name "lowpassfilterfreq" ;
      lv2:default 8000.000000 ;
      lv2:minimum 20.000000 ;
      lv2:maximum 20000.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 6 ;
      lv2:symbol "IMAGEPAN" ;
      lv2:name "imagepan" ;
      lv2:default 20.000000 ;
      lv2:minimum -90.000000 ;
      lv2:maximum 90.000000 ;
   ], [
      a lv2:InputPort ,
          lv2:ControlPort ;
      lv2:index 7 ;
      lv2:symbol "ORIGINALPAN" ;
      lv2:name "originalpan" ;
      lv2:default -20.000000 ;
      lv2:minimum -90.000000 ;
      lv2:maximum 90.000000 ;
   ] .


<urn:bill:monospace_ui>
   a guiext:X11UI;
   guiext:binary <monospace_ui.so> ;
       lv2:extensionData guiext::idle ;
       lv2:extensionData guiext:resize ;
       lv2:extensionData guiext:idleInterface ;
       lv2:requiredFeature guiext:idleInterface ;
       lv2:optionalFeature opts:options ;
       opts:supportedOption guiext:scaleFactor ; .
